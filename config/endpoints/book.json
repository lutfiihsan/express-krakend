{
	"books_route_group": [
		{
			"@comment": "login authentication",
			"endpoint": "/login",
			"method": "POST",
			"concurrent_calls": 1,
			"headers_to_pass": ["Accept-Encoding", "Content-Type", "Content-Lenght", "Host", "Origin", "User-Agent"],
			"output_encoding": "no-op",
			"cache_ttl": "3600",
			"timeout": "7200ms",
			"backend": [
				{
					"host": ["http://book-service:3000"],
					"method": "POST",
					"url_pattern": "/api/v1/auth/login",
					"encoding": "no-op",
					"is_collection": false,
					"disable_host_sanitize": false,
					"timeout": "14400ms",
					"extra_config": {
						"github.com/devopsfaith/krakend-jsonschema": {
							"type": "object",
							"properties": {
								"status": {
									"type": "string"
								},
								"code": {
									"type": "integer"
								},
								"method": {
									"type": "string"
								},
								"message": {
									"type": "string"
								},
								"accessToken": {
									"type": "string"
								}
							},
							"required": ["status", "code", "method", "message"]
						}
					}
				}
			],
			"extra_config": {
				"github.com/devopsfaith/krakend-jsonschema": {
					"type": "object",
					"properties": {
						"email": {
							"type": "string",
							"format": "email"
						},
						"pasword": {
							"type": "string",
							"format": "password"
						}
					},
					"required": ["email", "password"]
				}
			}
		},
		{
			"@comment": "add new book data",
