						"github.com/devopsfaith/krakend-jsonschema": {
							"type": "object",
							"properties": {
								"status": {
									"type": "string"
								},
								"code": {
									"type": "integer"
								},
								"method": {
									"type": "string"
								},
								"message": {
									"type": "string"
								}
							},
							"required": ["status", "code", "method", "message"]
						}
					}
				}
			],
			"extra_config": {
				"github.com/devopsfaith/krakend-jsonschema": {
					"type": "object",
					"properties": {
						"bookName": {
							"type": "string"
						},
						"bookPrice": {
							"type": "integer"
						},
						"bookReleaseDate": {
							"type": "string",
							"format": "date-time"
						},
						"bookPublishDate": {
							"type": "string",
							"format": "date-time"
						},
						"bookLanguage": {
							"type": "array",
							"format": "string"
						},
						"bookIsbn": {
							"type": "integer"
						},
						"bookPublisher": {
							"type": "string"
						}
					},
					"required": ["bookName", "bookPrice", "bookReleaseDate", "bookPublishDate", "bookLanguage", "bookIsbn", "bookPublisher"]
				}
			}
		},
		{
			"@comment": "results all book data",
