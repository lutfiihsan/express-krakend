{
	"version": 2,
	"name": "Express API Gateway Production",
	"max_idle_connections": 200,
	"extra_config":{
		{{ include "krakend_cors.tpl" }},
		{{ include "krakend_gobreaker.tpl" }},
		{{ include "krakend_gologging.tpl" }},
		{{ include "krakend_proxy.tpl" }}
	},
	"endpoints": [
		{{ range .test.route_group }}
		{
			"endpoint": "{{ .endpoint }}",
			"method": "{{ .method }}",
			"concurrent_calls": "{{ .concurrent_calls }}",
			"headers_to_pass": {{ template .headers_to_pass }},
			"output_encoding": "{{ .output_encoding }}",
			"cache_ttl": "{{ .cache_ttl }}",
			"timeout": "{{ .timeout }}"
		}
		{{ end }}
	]
}
